{
  "provider": {
    "name": "Pizza-Service"
  },
  "consumer": {
    "name": "FrontendApplication"
  },
  "interactions": [
    {
      "description": "get all pizzas",
      "request": {
        "method": "GET",
        "path": "/pizzas"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=UTF-8"
        },
        "body": [
          {
            "size": 8,
            "price": 9.99,
            "name": "Pizza Salami",
            "toppings": [
              {
                "pizza": "Pizza Salami",
                "price": 0.15,
                "name": "Salami",
                "weight": 10,
                "id": 10
              }
            ],
            "id": 1
          }
        ],
        "matchingRules": {
          "body": {
            "$": {
              "matchers": [
                {
                  "match": "type",
                  "min": 0
                }
              ],
              "combine": "AND"
            },
            "$[*].name": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$[*].id": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$[*].size": {
              "matchers": [
                {
                  "match": "number"
                }
              ],
              "combine": "AND"
            },
            "$[*].price": {
              "matchers": [
                {
                  "match": "decimal"
                }
              ],
              "combine": "AND"
            },
            "$[*].toppings": {
              "matchers": [
                {
                  "match": "type",
                  "min": 0
                }
              ],
              "combine": "AND"
            },
            "$[*].toppings[*].id": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$[*].toppings[*].name": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$[*].toppings[*].pizza": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$[*].toppings[*].weight": {
              "matchers": [
                {
                  "match": "number"
                }
              ],
              "combine": "AND"
            },
            "$[*].toppings[*].price": {
              "matchers": [
                {
                  "match": "decimal"
                }
              ],
              "combine": "AND"
            }
          },
          "header": {
            "Content-Type": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "get all pizzas"
        }
      ]
    },
